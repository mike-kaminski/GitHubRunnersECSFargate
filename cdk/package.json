{
  "name": "githubrunner",
  "version": "v0.0.1-alpha",
  "description": "Self-Hosted GitHub Runners",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk-diff-ops-dev": "npx cdk diff \"*\" -c config=ops-dev",
    "cdk-deploy-ops-dev": "npx cdk deploy \"*\" -c config=ops-dev",
    "cdk-destroy-ops-dev": "npx cdk destroy \"*\" -c config=ops-dev"
  },
  "devDependencies": {
    "@types/node": "^18.11",
    "aws-cdk": "^2.50",
    "cdk-ecr-deployment": "^2.5.6",
    "prettier": "^2.7",
    "ts-node": "^10.9",
    "typescript": "^4.8"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.50",
    "constructs": "^10.1"
  }
}
